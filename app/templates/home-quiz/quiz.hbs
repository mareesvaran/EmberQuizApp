
<div id="box">
  <div id="question">{{model.question}}</div>
</div>
<div class="Mari">
  {{#each model.option as |options|}}
  <div class="opt {{if (eq options model.check) 'copt'}}" {{action "answer" options}}>{{options}}
  </div>
  <!-- <input type="checkbox" class="check" onchange={{!--action "checkanswer" event--}} checked={{!--ischecked--}} value="{{!--value--}}"/>{{!--options--}}</div> -->
{{/each}}
<button  name="button" class="prev pn" {{action "prev"}}> PREV </button>
<button  name="button" class="next pn" {{action "next"}}> NEXT </button>
</div>
